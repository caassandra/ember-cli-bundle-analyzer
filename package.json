{
  "name": "ember-cli-bundle-analyzer",
  "version": "0.0.4",
  "description": "Analyze the size and contents of your Ember app's bundles",
  "keywords": [
    "ember-addon",
    "bundlesize",
    "bundle-analyzer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliber5/ember-cli-bundle-analyzer"
  },
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "mocha node-tests/**/*-test.js --recursive"
  },
  "dependencies": {
    "broccoli-concat-analyser": "^4.3.2",
    "debug": "^3.1.0",
    "ember-cli-babel": "^6.16.0",
    "ember-cli-version-checker": "^2.1.2",
    "hash-files": "^1.1.1",
    "intercept-stdout": "^0.1.2",
    "node-html-light": "^1.0.0",
    "sane": "^3.0.0",
    "tmp": "^0.0.33",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "ember-cli": "~3.5.1",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-load-initializers": "^1.1.0",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "history-support-middleware",
    "demoURL": "https://cdn.rawgit.com/kaliber5/ember-cli-bundle-analyzer/bceb55a7/docs/demo.html"
  }
}
